value="257"/><column index="44" value="197"/><column index="45" value="273"/><column index="46" value="119"/><column index="47" value="88"/><column index="48" value="132"/><column index="49" value="86"/><column index="50" value="105"/><column index="51" value="105"/><column index="52" value="110"/><column index="53" value="122"/><column index="54" value="160"/><column index="55" value="144"/><column index="56" value="113"/><column index="58" value="154"/><column index="59" value="197"/><column index="60" value="110"/><column index="61" value="185"/><column index="62" value="261"/><column index="63" value="197"/><column index="64" value="119"/><column index="65" value="274"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1">SELECT location, date, population, new_cases, total_cases, total_deaths
FROM deaths

-- lets look at total cases vs total deaths (lets find the percentage)

SELECT location, date, total_cases, total_deaths, ((total_deaths/total_cases)*100)
FROM deaths

SELECT location, date, total_cases, population, ((total_cases/population)*100)
FROM deaths

SELECT location, population, total_cases, (max(total_cases/population)*100)
FROM deaths

SELECT location, population, total_cases, (max(total_cases/population)*100)
FROM deaths
group by location, population
ORDER by (max(total_cases/population)*100) DESC

SELECT location, max((total_deaths ))
FROM deaths
WHERE continent is not NULL
group by location
ORDER by max(total_deaths) DESC

SELECT *
FROM deaths
WHERE continent is not NULL

SELECT location, max((total_deaths))
FROM deaths
WHERE continent is NULL
group by location
ORDER by max(total_deaths) DESC

SELECT date, sum(new_cases), sum(new_deaths)
FROM deaths
WHERE continent is not NULL
GROUP by date
ORDER by 1,2

SELECT date, sum(new_cases), sum(new_deaths), sum(new_deaths)/sum(new_cases)*100
FROM deaths
WHERE continent is not NULL
GROUP by date
ORDER by 1,2

-- lets get the sum of the gowth rate globally 
SELECT sum(new_cases), sum(new_deaths), sum(new_deaths)/sum(new_cases)*100
FROM deaths
WHERE continent is not NULL
ORDER by 1,2

SELECT *
FROM Vaccinations
ORDER by 1,2

SELECT *
FROM Vaccinations
JOIN deaths
    on deaths.location = Vaccinations.location
	AND deaths.date = Vaccinations.date

-- having joined the tables, lets find out what is the total vaccinations per continent
SELECT deaths.continent, deaths.location, deaths.population, Vaccinations.new_vaccinations,
 sum(Vaccinations.new_vaccinations) OVER (PARTITION by deaths.location ORDER by deaths.location,
  deaths.date) as RollingPeopleVaccinnated
FROM Vaccinations
JOIN deaths
    on deaths.location = Vaccinations.location
	AND deaths.date = Vaccinations.date
WHERE deaths.continent is not NULL
ORDER by 2, 3

-- create a view to store data for late

CREATE VIEW RollingPeopleVaccinnated as
SELECT deaths.continent, deaths.location, deaths.population, Vaccinations.new_vaccinations,
 sum(Vaccinations.new_vaccinations) OVER (PARTITION by deaths.location ORDER by deaths.location,
  deaths.date) as RollingPeopleVaccinnated
FROM Vaccinations
JOIN deaths
    on deaths.location = Vaccinations.location
	AND deaths.date = Vaccinations.date
WHERE deaths.continent is not NULL
</sql><current_tab id="0"/></tab_sql></sqlb_project>
